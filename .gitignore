//Print Odd or even number From List

fun main(){

   val numbers = listOf<Int>(1,2,3,4,5,6,7,8,9)

    var even_number = 0
    var odd_number = 0

    for (number in numbers){
        if (number % 2 == 0){
        even_number += 1
            println("$number is even number")
        } else {
        odd_number += 1
           
            println("$number is odd number")
        }
    }



}
